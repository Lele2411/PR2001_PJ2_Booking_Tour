<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-10">
    <div class="container">
      <div id="tour_messages">
        <h3><strong>All Tours</strong></h3>
        <table class="table table-hover" id="tours">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Category</th>
              <th>Price</th>
              <th>Counpon</th>
              <th>Seats</th>
              <th>Created at</th>
              <th>Last Update</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody>
            <%= render @tours %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
