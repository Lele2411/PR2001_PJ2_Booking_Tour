<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-10">
    <div class="container">
      <div id="hotel_messages">
        <h3><strong>All Hotels</strong></h3>
        <table class="table table-hover" id="hotels">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Location</th>
              <th>Link</th>
              <th>Created at</th>
              <th>Last Update</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody>
            <%= render @hotels %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
